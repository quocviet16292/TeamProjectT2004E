<script>
    $('.owl-carousel').owlCarousel({
        center:true,
        margin:10,
        loop:true,
        autoWidth:true,
        items:4,
        autoplay:true,
        autoplayTimeout:3000,
        autoplayHoverPause:true
    })

    //Alert Box

    function alert(){
        $('.alert').removeClass("hide");
        $('.alert').addClass("show");
        $('.alert').addClass("showAlert");
        setTimeout(function (){
            $('.alert').addClass("hide");
            $('.alert').removeClass("show");
        },3000)
    }
    $('.close-btn').click(function (){
        $('.alert').addClass("hide");
        $('.alert').removeClass("show");
    })
</script>

