        <div class="N_profile">
            <div class="N_profile_name">
                <%=v.Name %>
                <img src="/imgs/img/tichs%20do.png" style="max-width: 100%;height: 50px">
            </div>
            <div class="N_profile_aftername">Jakarta Selatan - DKI Jakarta</div>
