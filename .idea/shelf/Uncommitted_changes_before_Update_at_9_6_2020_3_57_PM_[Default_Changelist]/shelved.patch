me('display__total');\r\n    let total = 0;\r\n    for(let i = 0; i < document.getElementsByClassName('display__total').length; i++){\r\n        total += (parseFloat(document.getElementsByClassName('display__total')[i].innerText)*1000000);\r\n        //Loi so thap phan\r\n    }\r\n    console.log(total);\r\n// }\r\n//\r\n// sumCount();
